  {
    "subheading": "Clinical Information",
    "fields": [
      {
        "field_name": "Has the patient had prior therapy with the requested product within the last 365 days?",
        "type": "boolean",
        "options": [
          "Yes",
          "No"
        ]
      },
      {
        "field_name": "Has the patient had a trial and failure of any of the following rituximab biosimilars? (If yes, select all that apply)",
        "type": "multi_select_with_conditional",
        "options": [
          "No",
          "Ruxience (rituximab-pvvr)",
          "Truxima (rituximab-abbs)"
        ],
        "conditional_fields_on_select": [
          {
            "field_name": "When was the member's trial and failure of the preferred drug?",
            "type": "string",
            "condition": {
              "field": "Has the patient had a trial and failure of any of the following rituximab biosimilars? (If yes, select all that apply)",
              "not_value": "No"
            }
          },
          {
            "field_name": "Please describe the nature of the failure of the preferred drug",
            "type": "string",
            "condition": {
              "field": "Has the patient had a trial and failure of any of the following rituximab biosimilars? (If yes, select all that apply)",
              "not_value": "No"
            }
          }
        ]
      },
      {
        "field_name": "Has the patient had an adverse reaction to any of the following rituximab biosimilars? (If yes, select all that apply)",
        "type": "multi_select_with_conditional",
        "options": [
          "No",
          "Ruxience (rituximab-pvvr)",
          "Truxima (rituximab-abbs)"
        ],
        "conditional_fields_on_select": [
          {
            "field_name": "When was the member's adverse reaction to the preferred drug?",
            "type": "string",
            "condition": {
              "field": "Has the patient had an adverse reaction to any of the following rituximab biosimilars? (If yes, select all that apply)",
              "not_value": "No"
            }
          },
          {
            "field_name": "Please describe the nature of the adverse reaction to the preferred drug",
            "type": "string",
            "condition": {
              "field": "Has the patient had an adverse reaction to any of the following rituximab biosimilars? (If yes, select all that apply)",
              "not_value": "No"
            }
          }
        ]
      },
      {
        "field_name": "Please explain if there are any contraindications or other medical reason(s) that the patient cannot use any of the following preferred biosimilar products when indicated for the patient's diagnosis? (select all that apply)",
        "type": "multi_select",
        "options": [
          "Ruxience (rituximab-pvvr)",
          "Truxima (rituximab-abbs)"
        ]
      }
    ]
  }
